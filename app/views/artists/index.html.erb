<h2>Artists</h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Bio</th>
      <th>Start</th>
      <th>End</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @artists.each do |artist| %>
    <tr>
      <td><%= artist.name %></td>
      <td><%= artist.bio %></td>
      <td><%= artist.start_date %></td>
      <td><%= artist.end_date %></td>
      <td><%= link_to 'Display', artist %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br/>